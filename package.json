{
      "name": "ai-gaming-copilot",
      "version": "1.0.0",
      "main": "src/main.js",
      "scripts": {
        "start": "electron .",
        "dev": "vite",
        "build": "vite build --logLevel info",
        "train": "node scripts/train.js",
        "install:prod": "npm install --omit=dev --no-audit --no-fund",
        "ci": "npm install --omit=dev --no-audit --no-fund --prefer-offline",
        "postinstall": "electron-builder install-app-deps"
      },
      "dependencies": {
        "electron": "28.0.0",
        "onnxruntime-node": "^1.16.3",
        "puppeteer": "^22.8.2",
        "sharp": "^0.32.6",
        "ws": "^8.13.0",
        "fluent-ffmpeg": "^2.1.2",
        "ffmpeg-static": "^5.1.0",
        "lru-cache": "^10.0.0",
        "semver": "^7.6.3"
      },
      "devDependencies": {
        "vite": "^4.4.9",
        "electron-builder": "^24.6.4",
        "glob": "^10.3.10"
      }
    }
